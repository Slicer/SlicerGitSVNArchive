<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>Legacy.Converters</category>
  <title>BSpline to deformation field</title>
  <description><![CDATA[Create a dense deformation field from a bspline+bulk transform.]]></description>
  <version>0.1.0.$Revision: 2104 $(alpha)</version>
  <documentation-url>http://wiki.slicer.org/slicerWiki/index.php/Documentation/Nightly/Modules/BSplineToDeformationField</documentation-url>
  <license/>
  <contributor>Andrey Fedorov (SPL, BWH)</contributor>
  <acknowledgements><![CDATA[This work is funded by NIH grants R01 CA111288 and U01 CA151261.]]></acknowledgements>
  <parameters>
    <label>IO</label>
    <description><![CDATA[Input/output parameters]]></description>
    <transform>
      <name>inputTransformName</name>
      <label>BSpline transform that includes bulk affine transform</label>
      <longflag>--tfm</longflag>
      <channel>input</channel>
    </transform>
    <image>
      <name>referenceImageName</name>
      <label>Reference image</label>
      <longflag>--refImage</longflag>
      <channel>input</channel>
    </image>
    <image reference="inputTransformName">
      <name>deformationFieldName</name>
      <label>Output deformation field</label>
      <longflag>--defImage</longflag>
      <channel>output</channel>
    </image>
  </parameters>
</executable>
